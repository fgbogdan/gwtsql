<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:style>
		.cellTable {
		right: 0px;
		bottom: 0px;
		}
	</ui:style>

	<g:HTMLPanel>

		<iframe id="__printingFrame" style="width:0;height:0;border:0"></iframe>

		<div class="row">
			<div class="col-md-12">
				<div class="section-link section-link-first"></div>
				<div>
					<div class="page-section box">
						<div class="page-section-inner">
							<div class="page-section-header">
								<div class="page-section-title">
									<h2>
										<g:Label ui:field="lbTitle" />
									</h2>
								</div>
								<div class="page-section-menu">
									<div class="button-group-right">
										<table>
											<tr>
												<td>
													<g:FormPanel ui:field='pshbtnExportXLS'>
													</g:FormPanel>
												</td>
												<td>
													<g:Button ui:field="lnkPrint" styleName="bc-button bc-button-icon-small" title="Imprimare">
														<span class="bc-button-icon">
															<img alt="Imprimare" src="css/icons/bt-print-small-circle.png" />
														</span>
														<span class="bc-icon-over"></span>
													</g:Button>
												</td>
											</tr>
										</table>
									</div>
								</div>
							</div>
							<div class="block">
								<div class="block-header">
									<div class="block-title"></div>
									<div class="block-menu"></div>
								</div>
								<div class="block-body">
									<div class="tbl-data">
										<div id="printZone_1">
											<g:SimplePanel>
												<g:SimplePanel ui:field="pnlXLS" />
											</g:SimplePanel>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<iframe src="javascript:''" id="gwt-table-to-excel-target" tabIndex='-1' class="invisible" style="height:1px;width:1px;overflow:noscroll;"></iframe>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>

